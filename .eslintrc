{
    "plugins": ["react"],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "jest": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "rules": {
      "react/prop-types": 0,
      "no-async-promise-executor": 0,
      "no-console":0,
      "no-unused-vars":0,
      "react/display-name":0,
      "react/no-unescaped-entities":0,
      "no-undef":0
    },
    "settings": {
      "react": {
        "version": "16.8.6"
      }
    }
  }
  